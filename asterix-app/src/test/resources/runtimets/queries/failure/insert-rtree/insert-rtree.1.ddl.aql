drop dataverse test if exists;

create dataverse test;

use dataverse test;

create type MyRecord as closed {
	id: int32,
	tweetid: int64,
	loc: point,
	time: datetime,
	text: string
}

create type MyMiniRecord as closed {
  id: int32,
  loc: point
}

create dataset MyData(MyRecord)
  primary key id;

create dataset MyMiniData(MyMiniRecord)
  primary key id;

